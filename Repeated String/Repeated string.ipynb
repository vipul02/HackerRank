    "def count_a(s, n):\n",
    "    i = 0\n",
    "    s_n = []\n",
    "    while(i < n):\n",
    "        for char in 'aba':\n",
    "            s_n.append(char)\n",
    "            i += 1\n",
    "            if i == n:\n",
    "                break\n",
    "    s_n = ''.join(s_n)\n",
    "    print(s_n)\n",
    "    return sum([1 for a in s_n if a == 'a'])\n",
